}
@article{Pritchard2011a,
abstract = {Climate changes will influence soil organisms both directly (warming) and indirectly (warming and elevated CO2) via changes in quantity and quality of plant-mediated soil C inputs. Elevated atmospheric CO2 commonly stimulates flow of organic C into the soil system, increases root production and exudation, but decreases litter quality. There is little evidence that atmospheric CO2 enrichment will increase total soil organic matter content because greater C flow into soil stimulates the soil food web, often leading to equivalent increases in soil CO2 efflux. Effects of warming on C allocation belowground, on the other hand, will depend largely on the temperature optima of different plant species. Warming is likely to increase the rate of soil organic matter decomposition by stimulating soil heterotrophic respiration, although some degree of acclimatization to warming is likely. Mycorrhizal and N2-fixing relationships are generally enhanced by CO2 enrichment, but effects of warming are highly variable. Data suggest that energy flow through fungal pathways may be enhanced relative to bacterial pathways by both warming and atmospheric CO2 enrichment. Whether the shift toward fungal domination of soils will increase soilborne fungal disease occurrence in the future is still an open question. Plant heat and drought tolerance, along with resistance to pathogens in warmer and wetter soils, may be achieved, to some unknown extent, by exploitation and management of beneficial soil organisms. Further study is needed to develop a more holistic understanding of the effects of climate change on belowground processes.},
annote = {It is currently unclear how rising atmospheric CO2 will affect fungal and bacterial soilborne diseases because there are simply too few data on this topic. Both increases and decreases in soilborne diseases might be predicted based on documented effects of elevated CO2 on plant and canopy processes.

In a recent study on FACEgrown soybean, there was no effect of CO2 or O3 treatments on sudden death syndrome caused by the soil pathogen Fusarium. virguliforme (Eastburn et al., 2010); it should be noted that foliar, not root, symptoms were used for diagnostic purposes in that study.},
author = {Pritchard, S G},
doi = {10.1111/j.1365-3059.2010.02405.x},
file = {:C$\backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2011/2011{\_}Pritchard{\_}Soil organisms and global climate change.pdf:pdf},
isbn = {1365-3059},
journal = {Plant Pathology},
keywords = {biocontrol,elevated CO2,fungal ecology,global warming,soil food web,soilborne pathogens},
number = {1},
pages = {82--99},
publisher = {Blackwell Publishing Ltd},
title = {{Soil organisms and global climate change}},
url = {http://dx.doi.org/10.1111/j.1365-3059.2010.02405.x},
volume = {60},
year = {2011}
}
@article{Lonhardbory1992a,
abstract = {At Keszthely on Ramann's brown forest soil in 1987 the effect of increasing N-fertilizer rate was tested for the development of wheat leaf area, and its relation with the spreading of leaf- and stem base diseases. In the trial four N-levels (87, 174, 261, 348 kg/ha were applied. The basic fertilizer contained 140 kg/ha P2O5 and 140 kg/ba K2O. Indicator plant was wheat variety Mv-8. The leaf area was measured by LI 3000 Portable Area Meter. The degree of infection by leaf diseases (Erysiphe graminis, Puccinia triticina), and by stem base diseases (Gaeumannomyces graminis and Pseudocercosporella herpotrichoides) was reported in {\%}. The growing N-doses increased essentially the leaf area size, so besides the composition changes in leaf an ideal microclimate was created for the pathogens. With the applied N-doses a significant linear correlation (r=0,8765**) could be recorded between leaf area size and infection by Erysiphe graminis. Between infection by Puccinia triticina and leaf area size also a significant linear correlation (r=0,9435***) was found. Between infection by Gaeumannomyces graminis and Pseudocercosporella and leaf area there was also a close (r=0,9751***) linear correlation.},
