issn = {0031-949X},
journal = {Phytopathology},
month = {jun},
number = {6},
pages = {680--687},
pmid = {18944292},
title = {{A Test of Taxonomic Predictivity: Resistance to Early Blight in Wild Relatives of Cultivated Potato}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18944292 http://apsjournals.apsnet.org/doi/10.1094/PHYTO-98-6-0680},
volume = {98},
year = {2008}
}
