file = {:C$\backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2007/2007{\_}Verheggen et al.{\_}Electrophysiological and behavioral activity of secondary metabolites in the confused flour beetle, tribolium conf.pdf:pdf},
isbn = {0098-0331},
issn = {00980331},
journal = {Journal of Chemical Ecology},
keywords = {Aggregation pheromone,Behavior,Benzoquinones,Chemical communication,Dimethyldecanal,Electroantennography (EAG),Hydrocarbons,Tenebrionidae,Tribolium confusum},
number = {3},
