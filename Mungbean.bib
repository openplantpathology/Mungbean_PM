@Article{Thompson1993a,
  author   = {Thompson, G. B. and Brown, J. K. M. and Woodward, F. I.},
  journal  = {Plant Cell and Environment},
  title    = {{THE EFFECTS OF HOST CARBON-DIOXIDE, NITROGEN AND WATER-SUPPLY ON THE INFECTION OF WHEAT BY POWDERY MILDEW AND APHIDS}},
  year     = {1993},
  number   = {6},
  pages    = {687--694},
  volume   = {16},
  abstract = {In two experiments, winter wheat (Triticum aestivum cv. Cerco) was grown in 350 (ambient) and 700 mumol mol-1 (elevated) atmospheric CO2 concentrations. In the first experiment, plants were grown at five levels of nitrogen fertilization, and in the second experiment, plants were grown at three levels of water supply. All plants were infected with powdery mildew, caused by the fungus Erysiphe graminis. Plants grown in elevated atmospheric CO2 concentrations had significantly reduced {\%} shoot nitrogen contents and significantly increased {\%} shoot water contents. At elevated atmospheric CO2 concentrations, where plant nitrogen content was significantly reduced, the severity of mildew infection was significantly reduced, and where host water content was significantly increased, the severity of mildew infection was significantly increased. In a moderate water supply treatment, the plants grown in elevated atmospheric CO2 concentrations had significantly reduced nitrogen contents (9.9{\%}) and significantly increased water content (4{\%}), the amount of mildew infection was unchanged. The severity of mildew infection appeared to be more sensitive to host water content than to host nitrogen content.},
  annote   = {ISI Document Delivery No.: LW930 Times Cited: 44 Cited Reference Count: 43 THOMPSON, GB BROWN, JKM WOODWARD, FI Woodward, Ian/B-7762-2008 46 2 6 BLACKWELL SCIENCE LTD OXFORD PLANT CELL ENVIRON},
  doi      = {10.1111/j.1365-3040.1993.tb00487.x},
  isbn     = {0140-7791},
  keywords = {ATMOSPHERIC CO2,BARLEY,CO2,CO2 ENRICHMENT,ENRICHMENT,ERYSIPHE-GRAMINIS,F-SP HORDEI,FERTILIZATION,GRAIN-YIELD,HERBIVORE,INSECT,NITROGEN CONTENT,PLANT-GROWTH,POWDERY MILDEW,Plant Sciences,SEEDLING GROWTH,TRITICUM-AESTIVUM,WATER CONTENT,WHEAT,WINTER-WHEAT},
