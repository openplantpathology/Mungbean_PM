isbn = {0931-1785},
issn = {09311785},
journal = {Journal of Phytopathology},
keywords = {Beauvericin,Fusarium culmorum,Fusarium graminearum,Fusarium verticillioides,RAPD fingerprints,Species identification},
title = {{Species-specific PCR assays for the fungal pathogens Fusarium moniliforme and Fusarium subglutinans and their application to diagnose maize ear rot disease}},
