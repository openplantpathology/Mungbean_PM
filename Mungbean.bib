issn = {13601385},
keywords = {biotic stress,high CO2,pathogenesis-related responses,photorespiration,reactive oxygen species,salicylic acid},
title = {{Climate Change, CO2, and Defense: The Metabolic, Redox, and Signaling Perspectives}},
year = {2017}
}
