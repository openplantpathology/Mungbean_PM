  title    = {{Tri13 and Tri7 determine deoxynivalenol- and nivalenol-producing chemotypes of Gibberella zeae}},
  year     = {2002},
  number   = {5},
  pages    = {2148--2154},
  volume   = {68},
  abstract = {Gibberella zeae, a major cause of cereal scab, can be divided into two chemotypes based on production of the 8-ketotrichothecenes deoxynivalenol (DON) and nivalenol (NIV). We cloned and sequenced a Tri13 homolog from each chemotype. The Tri13 from a NIV chemotype strain (88-1) is located in the trichothecene gene cluster and carries an open reading frame similar to that of Fusarium sporotrichioides, whereas the Tri13 from a DON chemotype strain (H-11) carries several mutations. To confirm the roles of the Tri13 and TK7 genes in trichothecene production by G. zeae, we genetically altered toxin production in 88-1 and H-11. In transgenic strains, the targeted deletion of Tri13 from the genome of 88-1 caused production of DON rather than NIV. Heterologous expression of the 88-1 Tri13 gene alone or in combination with the 88-1 Tri7 gene conferred on H-11 the ability to synthesize NIV; in the latter case, 4-acetylnivalenol (4-ANIV) also was produced. These results suggest that Tri13 and Tri7 are required for oxygenation and acetylation of the oxygen at C-4 during synthesis of NIV and 4-ANIV in G. zeae. These functional analyses of the Tri13 and Tri7 genes provide the first clear evidence for the genetic basis of the DON and NIV chemotypes in G. zeae.},
  annote   = {ISI Document Delivery No.: 548UB Times Cited: 88 Cited Reference Count: 45 Amer soc microbiology Washington},
  doi      = {10.1128/aem.68.5.2148-2154.2002},
  file     = {:C$backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2002/2002{_}Lee et al.{_}Tri13 and Tri7 determine deoxynivalenol- and nivalenol-producing chemotypes of Gibberella zeae.pdf},
  isbn     = {0099-2240},
  keywords = {barley,fusarium-sporotrichioides encodes,graminearum,mycotoxins,natural occurrence,pathogenicity,regulatory gene,trichothecene biosynthesis,wheat,zearalenone production},
  language = {English},
  url      = {http://aem.asm.org/cgi/reprint/68/5/2148.pdf},
}

@Article{Lemmens2005a,
  author   = {Lemmens, M. and Scholz, U. and Berthiller, F. and Dall'Asta, C. and Koutnik, A. and Schuhmacher, R. and Adam, G. and Buerstmayr, H. and Mesterhazy, A. and Krska, R. and Ruckenbauer, P.},
  journal  = {Molecular Plant-Microbe Interactions},
