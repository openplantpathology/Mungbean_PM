  isbn     = {0028-646X},
  keywords = {FACE (free air CO(2),Rubisco,aspen populus-tremuloides,atmospheric change,carbon-dioxide enrichment,crop yield,deciduous forest canopy,elevated [CO(2)],elevated atmospheric co2,enrichment),field-grown cotton,leaf area,mojave desert shrubs,photosynthesis,potato solanum-tuberosum,regimes,relations,soil-nitrogen,source-sink,trees liquidambar-styraciflua},
