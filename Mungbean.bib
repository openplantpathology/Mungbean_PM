isbn = {0023-6152},
journal = {Journal of the Faculty of Agriculture Kyushu University},
number = {1},
pages = {289--300},
title = {{Effects of irrigation and nitrogen fertilization on photosynthesis, leaf area index and dry matter production of wheat on a clay terrace soil of Bangladesh}},
volume = {45},
year = {2000}
}
