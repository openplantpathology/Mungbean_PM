@Article{Lutz2010a,
  author   = {Lutz, Wolfgang and Samir, K. C.},
  journal  = {Philosophical Transactions of the Royal Society B: Biological Sciences},
  title    = {{Dimensions of global population projections: what do we know about future population trends and structures?}},
  year     = {2010},
  number   = {1554},
  pages    = {2779--2791},
  volume   = {365},
  abstract = {The total size of the world population is likely to increase from its current 7 billion to 8--10 billion by 2050. This uncertainty is because of unknown future fertility and mortality trends in different parts of the world. But the young age structure of the population and the fact that in much of Africa and Western Asia, fertility is still very high makes an increase by at least one more billion almost certain. Virtually, all the increase will happen in the developing world. For the second half of the century, population stabilization and the onset of a decline are likely. In addition to the future size of the population, its distribution by age, sex, level of educational attainment and place of residence are of specific importance for studying future food security. The paper provides a detailed discussion of different relevant dimensions in population projections and an evaluation of the methods and assumptions used in current global population projections and in particular those produced by the United Nations and by IIASA.},
  doi      = {10.1098/rstb.2010.0133},
  file     = {:C$backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2010/2010{_}Lutz, KC{_}Dimensions of global population projections what do we know about future population trends and structures.pdf},
  url      = {http://rstb.royalsocietypublishing.org/content/365/1554/2779.abstract},
}

@Article{Ma2005a,
  author   = {Ma, HongLiang and Zhu, JianGuo and Xie, ZuBin and Liu, Gang and Zhang, YaLi and Zeng, Qing},
  journal  = {Acta Agronomica Sinica},
  title    = {{Effects of free-air carbon dioxide enrichment on growth and uptake of nitrogen in winter wheat}},
  year     = {2005},
  number   = {12},
  pages    = {1634--1639},
  volume   = {31},
  abstract = {An experiment was conducted to study the effects of free-air CO2 enrichment on the growth and N uptake of winter wheat. The treatments comprised: CO2 at elevated and ambient conditions; and N at 150 and 250 kg/ha. The increase in wheat biomass averaged between 28.3 and 44.5{\%}, where the response was significant and highest at jointing stage. Under low and high N conditions, the biomass for leaves, shoots, ears and roots increased significantly by 36.8 and 40.9{\%}, 91.2 and 53.2{\%}, 89.4 and 52.2{\%}, and 55.5 and 67.9{\%}, respectively. N content in wheat decreased by 3.6-25.3{\%}, on average, due to elevated CO2 at both N levels and decreased more at low than at high N conditions. On the other hand, N uptake significantly increased by 20.8 and 29.2{\%} at low and high N, respectively, at jointing stage and by 1.6-32.4{\%} at other growth stages. Under elevated CO2 conditions, the relative absorption rate was 37.6{\%} at high N conditions, while the opposite was observed under ambient conditions at jointing stage.},
  annote   = {Cited Reference Count: 24 ref. English Science Press Beijing China},
  file     = {:C$backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2005/2005{_}Ma et al.{_}Effects of free-air carbon dioxide enrichment on growth and uptake of nitrogen in winter wheat.pdf},
  isbn     = {0496-3490},
  keywords = {7727-37-9,Amendments,Biochemistry,Cyperales,FF005Field Crops (NEW March 2000),FF060Plant Physiology and,FF061Plant Nutrition,JJ700Fertilizers and other,PP500Meteorology and Climate,Poaceae,Spermatophyta,Triticum,Triticum aestivum,angiosperms,biomass,carbon dioxide enrichment,crop growth stage,eukaryotes,growth,leaves,monocotyledons,nitrogen,nitrogen fertilizers,nutrient uptake,plant development,plants,roots,shoots,spikes,wheat,winter wheat},
  language = {Chinese},
