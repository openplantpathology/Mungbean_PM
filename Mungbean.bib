journal = {Food Research International},
keywords = {Aflatoxins,Climate change,Deoxynivalenol,Fungi,Mycotoxins,Ochratoxin A,Temperature,Water activity},
number = {7},
pages = {1902--1914},
title = {{How will climate change affect mycotoxins in food?}},
volume = {43},
