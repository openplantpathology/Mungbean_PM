booktitle = {Australian Journal of Agricultural Research},
file = {:C$\backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/1969/1969{\_}Purss{\_}The relationship between strains of Fusarium graminearum schwabe causing crown rot of various gramineous hosts and stalk rot.pdf:pdf},
pages = {257--64},
title = {{The relationship between strains of Fusarium graminearum schwabe causing crown rot of various gramineous hosts and stalk rot of maize in Queensland}},
