  file     = {:C$backslash$:/Users/U8011054/OneDrive - USQ/Cloudstor/Journal articles/Mendeley/2006/2006{_}Huang et al.{_}Temperature and leaf wetness duration affect phenotypic expression of Rlm6-mediated resistance to Leptosphaeria macula.pdf},
  isbn     = {0028-646X},
  keywords = {Brassica napus (oilseed rape),GFP (green fluorescent,a-group,ascospores,avirulence,england,epidemiology,europe,genes,juncea,major resistance gene,phoma stem canker,protein),recombinant lines resistant,temperature-sensitivity,winter oilseed rape},
